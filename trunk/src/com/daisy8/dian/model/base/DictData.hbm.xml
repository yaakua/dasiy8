<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.daisy8.dian.model.base.DictData" table="BASE_DICT_DATA">
    <id name="id">
      <generator class="identity"/>
    </id>
    <property length="16" name="name"/>
    <property name="disabled"/>
    <property length="8" name="code"/>
    <property name="remark"/>
    <many-to-one class="com.daisy8.dian.model.base.DictData" name="parent"/>
  </class>
</hibernate-mapping>
